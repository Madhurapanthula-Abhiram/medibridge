{
    "name": "medibridge-monorepo",
    "version": "1.0.0",
    "description": "MediBridge - Healthcare Platform with AI-powered features",
    "private": true,
    "scripts": {
        "install:all": "npm install --prefix frontend && npm install --prefix backend",
        "dev:frontend": "npm run dev --prefix frontend",
        "dev:backend": "npm start --prefix backend",
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "build:frontend": "npm run build --prefix frontend",
        "start:frontend": "npm run preview --prefix frontend",
        "start:backend": "npm start --prefix backend",
        "clean": "rm -rf frontend/node_modules backend/node_modules frontend/dist"
    },
    "keywords": [
        "healthcare",
        "medical",
        "ai",
        "symptom-checker",
        "doctor-finder"
    ],
    "author": "Abhiram Madhurapanthula",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Madhurapanthula-Abhiram/medibridge.git"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}